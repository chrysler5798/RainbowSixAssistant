    <!--OPERATORS UNIQUE GADGET DESCRIPTION-->
    <string name="ugat_ash">M120 CREM</string>
    <string name="uga_ash">Broń która wystrzeliwuje pocisk wbijający śie w powierzchnie,i wybuchając automatycznie.</string>

    <string name="ugat_bandit">CED-1</string>
    <string name="uga_bandit">Wysoko natężona maszynka która podpięta pod gadgety metaliczne powoduje prąd zadający obrażenia ludzią i gadgetą.</string>

    <string name="ugat_blackbeard">TARS MK-0 Tarcza</string>
    <string name="uga_blackbeard">2 Tarcze które można zamontowac na uchwyt broni, które bronią operatora.</string>

    <string name="ugat_blitz">G52-Tarcza Taktyczna</string>
    <string name="uga_blitz">Pozwala odpalic potężny błysk wbudowany w tarcze balistyczną który momentalnie oślepia wrogów.</string>

    <string name="ugat_buck">SK 4–12</string>
    <string name="uga_buck">Małą flinta podczepiona pod lufę głownej broni.</string>

    <string name="ugat_capitao">TAC Mk0</string>
    <string name="uga_capitao">Kusza taktyczna która wystrzeliwuje pociski duszące i micro granaty dymne.</string>

    <string name="ugat_castle">UTP1-Uniwersalny Panel Taktyczny</string>
    <string name="uga_castle">Kulo odporny panel który można nałożyć na każde okno i drwi. Bardziej wytrzymały nisz standardowe.</string>

    <string name="ugat_caveira">Cichy Chód</string>
    <string name="uga_caveira">Cichy chód CAVEIRY pozwala jej na ciche powalanie wroga i następne pszesłuchanie go, co pozwala na zlokalizowanie całej drużyny przeciwnej. Wróg zostaje potem eliminowany.</string>

    <string name="ugat_doc">MPD-0 Pistolet Strykawkowy</string>
    <string name="uga_doc">Strzela na odległość strzykawki które mogą podnieść obalonych i uleczyc rannych. Może tez dodac wiecej bżycia operatorowi z FULL HP.</string>

    <string name="ugat_echo">YOKAI</string>
    <string name="uga_echo">YOKAI jest latającym dronem który umożliwia ogłuszenie wroga. Jest on też niewidzialny jak przyklej śię do płaskiej powieszchni. Daje tez podgląd kamery.</string>

    <string name="ugat_frost">Sterling MK2 LHT</string>
    <string name="uga_frost">Pułąpka mechaniczna która blokuje i powala wroga.</string>

    <string name="ugat_fuze">APM-6 "Matryoshka"</string>
    <string name="uga_fuze">Jest to ładunek który wbija śię w ściane przebijając ją i wypuszcza małe granaty do pomieszczenia.</string>

    <string name="ugat_glaz">Celownik HDS</string>
    <string name="uga_glaz">Pozwala na zbliżenie celownika broni z dodatkowym sensorem podświetlającym osoby na żółto, nawet przez granaty gazowe i dymne.</string>

    <string name="ugat_hibana">X-KAIROS</string>
    <string name="uga_hibana">Wyrzutnia 40mm pocisków odpalanych na odległoś, które palą przeszkode.</string>

    <string name="ugat_iq">RED MK III "Spectre"</string>
    <string name="uga_iq">Czujnik który wykrywa nawet za ścianami każdy jaką kolwiek elektronike w pobliżu operatora.</string>

    <string name="ugat_jackal">EYENOX MODEL III</string>
    <string name="uga_jackal">Hełm taktyczny kóry pozwala na zlokalizowanie wroga dzięki skannerowi odcisków stóp.</string>

    <string name="ugat_jager">ADS-MKIV "Magpie"</string>
    <string name="uga_jager">Używa systemu obrony aktywnej który niszczy nadlatujące granaty zanim te wybuchną.</string>

    <string name="ugat_kapkan">EDD MK II</string>
    <string name="uga_kapkan">Pułapki laserowe które można wwiercić w okna i drwi.</string>

    <string name="ugat_mira">Czarne Lustro Weneckie</string>
    <string name="uga_mira">Lustro które przebija powieszchnie i pozwala na widoczność za scianą. Moze zostac zniszczone w celu zrobiena dziury mordu.</string>

    <string name="ugat_montagne">Le Roc</string>
    <string name="uga_montagne">Tarcza która ma możliwość powiększenia śię do rozmiaru operatora w tym broniąc jego przednią część ciała.</string>

    <string name="ugat_mute">GC90 Moni Zakłócacz Sygnału</string>
    <string name="uga_mute">Zakłóca sygnał aparatów zdalnie sterowanych i detonowanych.</string>

    <string name="ugat_pulse">HB-5 Wykrywacz Pulsu</string>
    <string name="uga_pulse">Wykrywa bicie serca wroga na bliskich dystansach.</string>

    <string name="ugat_rook">R1N "Rhino" Armor</string>
    <string name="uga_rook">Paczka kamizelek kulo odpornych rozdawana drużynie. Zmniejsza obrażenia dostawane.</string>

    <string name="ugat_sledge">Taktyczny Młot Wyłomowy "The Caber"</string>
    <string name="uga_sledge">Młot który jest w stanie rozbic każdą zniszczalną powierzchnie.</string>

    <string name="ugat_smoke">Z8 Granat Gazowy</string>
    <string name="uga_smoke">Zdalnie detonowane ładunki toksycznego gazu.</string>

    <string name="ugat_tachanka">RP-46 Degtyaryov Karabin Maszynowy</string>
    <string name="uga_tachanka">DRozkładany karabin który broni operatora tarczą balistyczną przed strałami frontowymi.</string>

    <string name="ugat_thatcher">EG MK0-EMP Granat</string>
    <string name="uga_thatcher">Granat wybuchowy powodujący fale EMP które niszczą elektronike w zaśięgu.</string>

    <string name="ugat_thermite">BRIMSTONE BC-3 Ładunek Exotermiczny</string>
    <string name="uga_thermite">Ładunek który przepala wzmocnione metalowo ściany.</string>

    <string name="ugat_twitch">RDS Model 1 Dron Porażający</string>
    <string name="uga_twitch">Dron który pozwala niszczyć pułapki i ranić wrogów. 1 dron jest używany podczas fazy przygotowawczej, 2 jest dostępny podczas gry.</string>

    <string name="ugat_valkyrie">Gyro Cam Mk2</string>
    <string name="uga_valkyrie">Czarne kamery ktore można ustawiac osobiście dla całej drużyny.</string>

    <string name="ugat_ying">Candela</string>
    <string name="uga_ying">Pakiet tadunków oślepiających, które mogą zostać przymocowane do powierzchni lub użyte jako granaty.</string>

    <string name="ugat_lesion">Gu</string>
    <string name="uga_lesion">Zamaskowana mina chemiczna, która wstrzykuje mieszaninę trujących chemikaliów, zadając poważne obrażenia i ograniczając szybkość poruszania się celu.</string>

    <string name="ugat_ela">Grzmot Mine</string>
    <string name="uga_ela">Mina ogłuszająca o działaniu kierunkowym, którą można zamocować do wybranej powierzchni, wywołująca poważne zaburzenia słuchu i dezorientację.</string>

    <string name="ugat_zofia">KS79 LIFELINE</string>
    <string name="uga_zofia">Podwójna wyrzutnia, z której można strzelać zarówno granatami ogłuszającymi, jak i uderzeniowymi.</string>

    <string name="ugat_dokkaebi">BOMBA LOGICZNA</string>
    <string name="uga_dokkaebi">Przesyła wirusa, który zmusza telefony przeciwnika do wydawania cichego brzęczenia, które można usłyszeć i dzięki temu wykryć pozycję wroga.</string>

    <string name="ugat_vigil">ERC-7</string>
    <string name="uga_vigil">Vigil skanuje okoliczne urządzenia, wymazując swój obraz z transmisji prowadzonych przez drony i kamery.</string>

