    <string name="ugat_ash">M120 CREM</string>
    <string name="uga_ash">Ispaljuje eksplozivni prodor koji se zakopa u površinu i automatski detonira.</string>

    <string name="ugat_bandit">CED-1</string>
    <string name="uga_bandit">Postavlja šok žicu velikog napona na metalne uređaje, nanošenjem štete neprijatelju.</string>

    <string name="ugat_blackbeard">TARS MK 0-Armored Rifle Shield</string>
    <string name="uga_blackbeard">Ima štit na svom primarnom oružju.</string>

    <string name="ugat_blitz">G52-Tactical Shield</string>
    <string name="uga_blitz">Koristi bljesak prikačen na njegov balistički štit da privremeno oslepi neprijatelje.</string>

    <string name="ugat_buck">SK 4–12</string>
    <string name="uga_buck">Uključuje dodatak pumparice ispod cevi na svom primarnom oružju.</string>

    <string name="ugat_capitao">TAC Mk0</string>
    <string name="uga_capitao">Taktički samostrel koji puca strele na udar i mikro dimne bombe.</string>

    <string name="ugat_castle">UTP1-Universal Tactical Panel</string>
    <string name="uga_castle">Postavlja barikade otporne na metak, otpornije od standardnih.</string>

    <string name="ugat_caveira">Silent step</string>
    <string name="uga_caveira">Njen pištolj joj dozvoljava da proganja neprijatelje, gotovo nečujno, sve dok nije kasno.</string>

    <string name="ugat_doc">MPD-0 Stim Pistol</string>
    <string name="uga_doc">Može da podigne sebe ili saigrače sa distance. Takođe može pružiti privremeni podsticaj.</string>

    <string name="ugat_echo">YOKAI</string>
    <string name="uga_echo">Leteći dron koji oslobađa dezorijentisane zvučne talase. Yokai takođe prenosi video zapise Echo-u i njegovom timu.</string>

    <string name="ugat_frost">Sterling MK2 LHT</string>
    <string name="uga_frost">Koristi mehaničku zamku kako bi onesposobila neprijatelje.</string>

    <string name="ugat_fuze">APM-6 "Matryoshka"</string>
    <string name="uga_fuze">Koristi grupna punjenja koja prolaze i drže zidove, oslobađajući granate na suprotnoj strani zida.</string>

    <string name="ugat_glaz">HDS Flip Sight</string>
    <string name="uga_glaz">Uključuje opseg uvećanja sa sofisticiranim senzorima slike koji obeležavaju mete velikog dometa, čak i kroz dim.</string>

    <string name="ugat_hibana">X-KAIROS</string>
    <string name="uga_hibana">40mm raketni bacač granata eksplozivnih kuglica koje se mogu detonirati sa daljine.</string>

    <string name="ugat_iq">RED MK III "Spectre"</string>
    <string name="uga_iq">Koristi Elektronski Detektor da locira bilo koju elektroniku u opsegu kroz prepreke kao što su zidovi.</string>

    <string name="ugat_jackal">EYENOX MODEL III</string>
    <string name="uga_jackal">Naočare za praćenje koje lociraju ciljeve otkrivajući i prepoznavajući najnovije korake.</string>

    <string name="ugat_jager">ADS-MKIV "Magpie"</string>
    <string name="uga_jager">Koristi aktivnu odbranu kako bi presreo granate pre nego što se detoniraju.</string>

    <string name="ugat_kapkan">EDD MK II</string>
    <string name="uga_kapkan">Postavlja zamke na prozorima i okvirima vrata.</string>

    <string name="ugat_mira">BLACK MIRROR</string>
    <string name="uga_mira">Postavlja jednosmerni prozor protiv metaka na lomljive i ojačane zidove. Takođe se može izbaciti da stvori rupu za ubistvo.</string>

    <string name="ugat_montagne">Le Roc</string>
    <string name="uga_montagne">Može da produži svoj štit za punu zaštitu dok stoji.</string>

    <string name="ugat_mute">GC90 "Moni" Signal Disruptor</string>
    <string name="uga_mute">Zamagljuje komunikaciju za daljinski detonirane naprave ili dronove u svom opsegu.</string>

    <string name="ugat_pulse">HB-5 Cardiac Sensor</string>
    <string name="uga_pulse">Detektuje otkucaje srca na maloj udaljenosti kroz prepreke.</string>

    <string name="ugat_rook">R1N "Rhino" Armor</string>
    <string name="uga_rook">Spušta torbu koja je puna oklopa za saigrače.</string>

    <string name="ugat_sledge">Tactical Breaching Hammer "The Caber"</string>
    <string name="uga_sledge">Upotrebljava čekić za razvaljivanje da probije kroz slabe površine.</string>

    <string name="ugat_smoke">Compound Z8 Grenade</string>
    <string name="uga_smoke">Postavlja daljinski detonirane toksične gasove.</string>

    <string name="ugat_tachanka">RP-46 Degtyaryov Machine Gun</string>
    <string name="uga_tachanka">Postavlja motorizovan LMG s balističkim štitom koji štiti glavu od frontalnih metaka.</string>

    <string name="ugat_thatcher">EG MK0-EMP Grenade</string>
    <string name="uga_thatcher">Koristi EMP granate kako bi onemogućio bilo koju elektroniku u opsegu.</string>

    <string name="ugat_thermite">BRIMSTONE BC-3 Exothermic Charge</string>
    <string name="uga_thermite">Postavlja egzotermično punjenje koje uništava ojačane zidove.</string>

    <string name="ugat_twitch">RSD Model 1 Shock Drone</string>
    <string name="uga_twitch">Koristi dva Šok Drona kako bi onemogućila zamke ili povredila neprijatelje. Koristi prvi Šok Dron tokom pripremne faze.</string>

    <string name="ugat_valkyrie">Gyro Cam Mk2</string>
    <string name="uga_valkyrie">Može postaviti kamere, dostupne celom timu pomoću alata za posmatranje.</string>

    <string name="ugat_ying">Candela</string>
    <string name="uga_ying">Grupa blic granata koje se mogu učvrstiti na površinama ili se baciti kao granata.</string>

    <string name="ugat_lesion">Gu</string>
    <string name="uga_lesion">Maskirane toksične mine ubrizgavaju otrov koji povređuje protivnike i ograničava njihovu brzinu.</string>

    <string name="ugat_ela">Grzmot Mine</string>
    <string name="uga_ela">Mine potresa koje se mogu zakačiti na površinama, oštećuju sluh i izazivaju vrtoglavicu.</string>

    <string name="ugat_zofia">KS79 LIFELINE</string>
    <string name="uga_zofia">Dvostruka cev koja može ispaliti i potresne granate i udarne granate.</string>

    <string name="ugat_dokkaebi">LOGIC BOMB</string>
    <string name="uga_dokkaebi">Prenosi virus koji prisiljava neprijateljske uređaje da emitiraju glasno zujanje, čime otkriva njihov položaj.</string>

    <string name="ugat_vigil">ELECTRONIC RENDERING CLOAK (ERC-7)</string>
    <string name="uga_vigil">Vigil skenira okolne uređaje kako bi obrisao njegovu sliku sa kamera i dronova.</string>

    <string name="ugat_lion">EE-ONE-D</string>
    <string name="uga_lion">The EE-ONE-D detects enemy movement, providing an overview of the battlefield.</string>

    <string name="ugat_finka">ADRENAL SURGE</string>
    <string name="uga_finka">The nanobots give the team a brief gain in HP and revives allies in a DBNO state.</string>

