    <!--OPERATORS UNIQUE GADGET DESCRIPTION-->
    <string name="ugat_ash">M120 CREM</string>
    <string name="uga_ash">Projette une charge dans une surface pour un maximum de destruction.</string>

    <string name="ugat_bandit">CED-1</string>
    <string name="uga_bandit">Place des fils électrifiés sur les gadgets métalliques, infligeant des dégâts aux ennemis.</string>

    <string name="ugat_blackbeard">Bouclier d\'arme blindé TARS MK 0</string>
    <string name="uga_blackbeard">Possède un bouclier de secours fixé à son arme, à utiliser si le premier est endommagé.</string>

    <string name="ugat_blitz">Bouclier tactique G52</string>
    <string name="uga_blitz">Déclenche le flash fixé sur le bouclier afin d\'aveugler temporairement l\'ennemi.</string>

    <string name="ugat_buck">SK 4–12</string>
    <string name="uga_buck">Actionne une arme de calibre 12 fixée sous son arme principale.</string>

    <string name="ugat_capitao">TAC Mk0</string>
    <string name="uga_capitao">Arbalète tactique tirant des carreaux qui libèrent un gaz asphyxiant, ainsi que des grenades fumigènes miniatures.</string>

    <string name="ugat_castle">Panneau tactique universel UTP1</string>
    <string name="uga_castle">Met en place des barricades plus résistantes.</string>

    <string name="ugat_caveira">Pas de velours</string>
    <string name="uga_caveira">La furtivité de Caveira "Pas de velours" lui permet de traquer ses proies presque sans bruit.</string>

    <string name="ugat_doc">Pistolet stim MPD-0</string>
    <string name="uga_doc">Effectue une injection hypodermique qui le réanime ou réanime un équipier à distance. Peut également octroyer un bonus temporaire.</string>

    <string name="ugat_echo">YOKAI</string>
    <string name="uga_echo">Drone volant qui peut émettre des décharges soniques désorientant les ennemis. Yokai fournit aussi une transmission vidéo à l\'agent et à son équipe.</string>

    <string name="ugat_frost">Sterling MK2 LHT</string>
    <string name="uga_frost">Utilise un piège mécanique pour neutraliser les ennemis.</string>

    <string name="ugat_fuze">APM-6 "Matryoshka"</string>
    <string name="uga_fuze">Charges murales à sous-munitions expédiant des mini-grenades au travers des parois.</string>

    <string name="ugat_glaz">Visée auxiliaire HDS</string>
    <string name="uga_glaz">Active une lunette grossissante dotée de capteurs d\'images high-tech, capable de marquer les cibles à longue portée, même dans les fumigènes.</string>

    <string name="ugat_hibana">X-KAIROS</string>
    <string name="uga_hibana">Lance-grenades de 40mm qui projette des plombs explosifs pouvant être déclenchés à distance.</string>

    <string name="ugat_iq">DSE MK III "Spectre"</string>
    <string name="uga_iq">Utilise un détecteur électronique pour localiser les appareils électroniques à portée.</string>

    <string name="ugat_jackal">EYENOX MODELE III</string>
    <string name="uga_jackal">Cet équipement pour les yeux permet de localiser une cible en révélant ses empreintes de pas et en les identifiant.</string>

    <string name="ugat_jager">ADS-MKIV "Magpie"</string>
    <string name="uga_jager">Système de défense active neutralisant les grenades avant leur détonation.</string>

    <string name="ugat_kapkan">DBA EDD MK II</string>
    <string name="uga_kapkan">Place des pièges explosifs sur les encadrements des portes et fenêtres.</string>

    <string name="ugat_mira">MIROIR NOIR</string>
    <string name="uga_mira">Déploie un miroir sans tain pare-balles sur les murs renforcés et susceptibles d\'être brisés. Peut également être éjecté pour créer un assomoir.</string>

    <string name="ugat_montagne">Le Roc</string>
    <string name="uga_montagne">Son bouclier extensible permet d\'obtenir une protection complète en position debout.</string>

    <string name="ugat_mute">Brouilleur de signaux GC90 "Moni"</string>
    <string name="uga_mute">Brouille les communications des gadgets électroniques ou des drones à sa portée.</string>

    <string name="ugat_pulse">Capteur cardiaque HB-5</string>
    <string name="uga_pulse">Détecte les battements cardiaques à courte distance, à travers les obstacles.</string>

    <string name="ugat_rook">Protections R1N "Rhino"</string>
    <string name="uga_rook">Fournit un sac rempli de plaques de protection à l\'équipe.</string>

    <string name="ugat_sledge">Masse Tactique "Le Tronc"</string>
    <string name="uga_sledge">Utilise une masse pour démolir les surfaces destructibles.</string>

    <string name="ugat_smoke">Grenade à composés Z8</string>
    <string name="uga_smoke">Libère du gaz toxique à distance.</string>

    <string name="ugat_tachanka">Mitrailleuse RP-46 Degtyaryov</string>
    <string name="uga_tachanka">Déploie une mitrailleuse statique dotée d\'un bouclier balistique protégeant des tirs frontaux.</string>

    <string name="ugat_thatcher">Grenade IEM EG Mk0</string>
    <string name="uga_thatcher">Utilise des grenades IEM pour neutraliser tout matériel électronique à portée.</string>

    <string name="ugat_thermite">Charge exothermique BRIMSTONE BC-3</string>
    <string name="uga_thermite">Utilise une charge capable de détruire les parois renforcées.</string>

    <string name="ugat_twitch">Drone à électrocution RSD Model 1</string>
    <string name="uga_twitch">Utilise deux drones à électrocution pour désactiver des pièges ou blesser des ennemis. Déploie le premier drone lors de la phase de préparation.</string>

    <string name="ugat_valkyrie">Caméra gyroscopique Mk2</string>
    <string name="uga_valkyrie">Peut paramétrer des caméras "Black Eye", accessibles par toute l\'équipe via les outils d\'observation.</string>

    <string name="ugat_ying">Candela</string>
    <string name="uga_ying">Nuée d\'explosifs à flashs pouvant être accolée à des surfaces ou lancée comme une grenade.</string>

    <string name="ugat_lesion">Gu</string>
    <string name="uga_lesion">Mines toxiques dissimulées injectant un composé qui inflige des dégâts à l\'adversaire et le ralentit.</string>

    <string name="ugat_ela">Grzmot Mine</string>
    <string name="uga_ela">Mines offensives de proximité pouvant être collées à des surfaces. Leur déclenchement assourdit et étourdit la cible.</string>

    <string name="ugat_zofia">KS79 LIFELINE</string>
    <string name="uga_zofia">Double barrel that can fire both concussion and impact grenades.</string>

    <string name="ugat_dokkaebi">LOGIC BOMB</string>
    <string name="uga_dokkaebi">Uploads a virus that forces enemy devices to emit a loud buzz, thereby compromising their position.</string>

    <string name="ugat_vigil">ELECTRONIC RENDERING CLOAK (ERC-7)</string>
    <string name="uga_vigil">Vigil scans surrounding devices to wipe his image from cameras and drones.</string>

